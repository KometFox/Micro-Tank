
ACTOR 75mmAP_ExplosionSpawner : Particle
{
  States
  {
  Spawn:
    TNT1 A 0
    Goto Death
  Death:
    TNT1 AAAAAAA 0 A_Spawnitemex("Dirtchunk_S0.75",0,0,0,0+random(-7,7),0+random(7,7),0+frandom(5,10),0+random(-360,360))
    TNT1 A 0 A_Spawnitem("Explosion0_S0.5")
    TNT1 A  0  A_SpawnItemEx("BigGlowSmall",0,0,0,0+frandom(-1,1),0+frandom(-1,1),0+frandom(-1,1),0+frandom(-360,360))
    TNT1 AAAAAA  0  A_spawnitemex("Smoke1_S0.75",0,0,0,0+frandom(-3,3),0+frandom(-3,3),0+frandom(1,3),0+frandom(-360,360))
    TNT1 A 0 A_Spawnitemex("MT_SparkyTrail",0,0,0,0+random(-15,15),0+random(-15,15),0+random(4,13),0+frandom(-360,360))
    Stop
  } 
}


