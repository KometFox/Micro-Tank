ACTOR MT_CEG_75mmHE_Trail : MT_PRT_TrailBase {
  Var Int User_TTL;
  Gravity 0
  States {
 Spawn:
    TNT1 A 1 A_SetUserVar("User_TTL",1)	
    goto Loopy  
  Loopy:
     APP1    A 2
    "####"   A 0 A_Spawnitemex("SmallGlow")
    "####"   A 0 A_Spawnitemex("Spark1",0,0,0,0,0,1)
    "####"   A 0 A_SetUserVar("User_TTL",User_TTL+1)
    "####"   A 0 A_JumpIf(User_TTL==15,"Death")
    loop
}}



