ACTOR MT_50mmThermoRocket_2_FuelCloud : MT_AreaCloud
{
  renderstyle Add
  BounceCount 8
  BounceType Doom
  WallBounceFactor 0.8
  DamageType Fire
  +FORCERADIUSDMG
  Gravity 0
  alpha 1
  scale 0.85
  radius 8
  height 8
  States
  {
  Spawn:
    TNT1 AAAAAAAA 6 A_SpawnItem("Smoke1_S0.5") 
    Goto Death
  Death:
    TNT1 A 0 A_Spawnitem("Explosion2_S1.25")
    TNT1 A 0 A_Explode(80,150)
    Goto Death2
  }
}



