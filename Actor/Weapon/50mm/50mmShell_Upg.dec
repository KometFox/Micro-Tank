  States
  {
  Death:
    TNT1 A 0  A_Spawnitem("50mmHE_ExplosionSpawner")
    TNT1 AA 0 A_SpawnItemEx("Shockwave0_S1.25")
    TNT1 A 0  A_Playsound("boom12")
    TNT1 A 4  A_Explode(230,176)
    Stop
  }
}

